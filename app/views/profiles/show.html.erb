<div class="location-style">
	<h1 class="friends-header"> Friends List</h1>
	
	<table>
		<th class="friend-name">Friends name</th>		
		<% @user.friendships.each do |friendship|%>
		<tr id="remove-<%= friendship.id %>">
			<td class="friend-remove"><%= friendship.friend.username %></li>
			<td><%= link_to "- Remove Friend", friendship,class: "btn btn-danger remove-friendship btn-md", remote: true, method: :delete, id: "remove-friend" %></span>
		</tr>
		<% end %>
	</ul>
</div>